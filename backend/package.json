{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec ts-node src/index.ts",
    "typeormold": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm": "ts-node -P ./tsconfig.typeorm.json -d ./src/db/CliDataSource.ts",

    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/datasource.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert -d ./src/datasource.ts",
    "schema:sync": "",
    "migration:show": "npm run typeorm migration:show -- -d ./src/datasource.ts",
    "migration:generate": "npm run typeorm migration:generate -- -d ./src/datasource.ts",
    
    "migration:create": "npm run typeorm migration:create"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.17",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@nestjs/typeorm": "^10.0.0",
    "express": "^4.18.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  }
}
